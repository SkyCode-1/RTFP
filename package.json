{
  "name": "rtfp",
  "version": "0.2.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkyCode-1/RTFP.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "ci:publish": "changeset publish"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.20.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.6"
  }
}
